{
    "extends": "standard",
    "env": {
        "node": true
    },
    "installedESLint": true, 
    "rules": {
        "semi": [2, "always"],
        "strict": 0,
        "indent": [2, 4, {
            "SwitchCase": 1
        }],
        "spaced-comment": 0,
        "one-var": [2, "always"],
        "no-multi-spaces": 0,
        "eol-last": 0,
        "curly": 0,
        "no-multiple-empty-lines": 2
    }
}
